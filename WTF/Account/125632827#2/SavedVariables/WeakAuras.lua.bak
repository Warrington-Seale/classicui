["frameStrata"] = 5,
["anchorFrameType"] = "SCREEN",
["customText"] = "function()\n    -- ─── throttle ──────────────────────────────────────────────────────────────\n    aura_env.last = aura_env.last or 0\n    local now = GetTime()\n    if now - aura_env.last < 0.2 then\n        -- too soon: return prior values\n        return aura_env.ret1, aura_env.ret2, aura_env.ret3, aura_env.ret4, aura_env.ret5, aura_env.ret6\n    end\n    aura_env.last = now\n    -- ─── end throttle ──────────────────────────────────────────────────────────\n    \n    local red   = \"ff0000\"\n    local green = \"00ff00\"\n    local white = \"ffffff\"\n    \n    -- grab raw data\n    local cCrit   = GetCritChance()\n    local cHit    = GetHitModifier() or 0\n    local tLevel  = UnitLevel(\"Target\")\n    if tLevel <= 0 then tLevel = 63 end\n    local cLevel  = UnitLevel(\"Player\")\n    \n    local mBase, mMod, oBase, oMod = UnitAttackBothHands(\"player\")\n    local apBase, apAdd, apSub      = UnitAttackPower(\"player\")\n    local _, _, _, _, _, _, dMod    = UnitDamage(\"player\")\n    \n    -- compute totals\n    local mhSkill = mBase + mMod\n    local ohSkill = oBase + oMod\n    local cAp     = apBase + apAdd + apSub\n    local cDmg    = math.ceil(dMod * 100)\n    \n    -- defense math\n    local tDef    = tLevel * 5\n    local cap     = cLevel * 5\n    local baseLim = math.min(cap, mhSkill)\n    local extra   = mhSkill - 300\n    local diff    = tDef - mhSkill\n    \n    -- miss\n    local missBase = (diff > 10) and (5 + diff * 0.2) or (5 + diff * 0.1)\n    local cMiss = IsDualWielding()\n    and ((missBase * 0.8 + 20) - cHit)\n    or (missBase - cHit)\n    \n    -- dodge, glance, crit cap\n    local tDodge  = 5 + diff * 0.1\n    local tGlance = math.max(10 + (tDef - baseLim) * 2, 0)\n    local sup     = 4.8\n    local cCap    = 100 - cMiss - tDodge - tGlance + sup + (extra * 0.04)\n    \n    -- format strings\n    local critStr = \"Crit: |r|cff\"\n    .. ((cCrit >= cCap) and red or green)\n    .. string.format(\"%.2f\", cCrit)\n    .. \"% |r|cff\" .. white\n    .. string.format(\"/ %.2f%%\", cCap)\n    \n    local hitStr = \"Hit: |r|cff\"\n    if mhSkill >= tDef - 15 then\n        local pass = (mhSkill < tDef - 10 and cHit >= 9) or (mhSkill >= tDef - 10 and cHit >= 6)\n        hitStr = hitStr .. (pass and green or red) .. cHit\n    else\n        hitStr = hitStr .. red .. cHit\n    end\n    hitStr = hitStr .. \"% |r|cff\" .. white\n    \n    -- **two-line** weapon skill\n    local skillStr =\n    \"MH Skill: |r|cff\" .. ((mhSkill >= tDef - 15) and green or red) .. mhSkill .. \"|r\\n\" ..\n    \"OH Skill: |r|cff\" .. ((ohSkill >= tDef - 15) and green or red) .. ohSkill .. \"|r\"\n    \n    local apStr  = \"Attack Power: \" .. cAp\n    local dmgStr = \"Damage: |r|cff\"\n    .. ((cDmg == 100 and white) or (cDmg > 100 and green) or red)\n    .. cDmg\n    \n    -- stash for throttle\n    aura_env.ret1, aura_env.ret2, aura_env.ret3, aura_env.ret4, aura_env.ret5, aura_env.ret6 =\n    cMiss .. \", \" .. tDodge .. \", \" .. tGlance .. \", \" .. sup .. \", \" .. (extra * 0.04),\n    critStr,\n    hitStr,\n    skillStr,\n    apStr,\n    dmgStr\n    \n    return aura_env.ret1, aura_env.ret2, aura_env.ret3, aura_env.ret4, aura_env.ret5, aura_env.ret6\nend",
